<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="AndesCheckbox" tools:ignore="ResourceName">

        <attr name="andesCheckboxAlign" format="enum">
            <enum name="left" value="1000" />
            <enum name="right" value="1001" />
        </attr>

        <attr name="andesCheckboxStatus" format="enum">
            <enum name="selected" value="2000" />
            <enum name="unselected" value="2001" />
            <enum name="undefined" value="2002" />
        </attr>

        <attr name="andesCheckboxType" format="enum">
            <enum name="idle" value="3000" />
            <enum name="disabled" value="3001" />
            <enum name="error" value="3002" />
        </attr>

        <attr name="andesCheckboxText" format="reference" />

    </declare-styleable>

</resources>