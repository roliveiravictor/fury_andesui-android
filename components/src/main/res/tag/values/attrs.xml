<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="AndesTagSimple" tools:ignore="ResourceName">
        <attr name="tagSimpleType">
            <enum name="neutral" value="2000" />
            <enum name="highlight" value="2001" />
            <enum name="success" value="2002" />
            <enum name="warning" value="2003" />
            <enum name="error" value="2004" />
        </attr>

        <attr name="tagSimpleSize">
            <enum name="large" value="3000" />
            <enum name="small" value="3001" />
        </attr>

        <attr name="tagSimpleIsDismissable">
            <enum name="True" value="4000" />
            <enum name="False" value="4001" />
        </attr>

        <attr name="tagSimpleText" format="reference" />
    </declare-styleable>
</resources>